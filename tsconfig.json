{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "types": [
    "@types/node",
    "@types/vuex-persistedstate",
  ],
  "typeRoots": [
    "./src"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}
